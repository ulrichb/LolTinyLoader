{
  "compilerOptions": {
    "outFile": "bin/ModulesSample.js",
    "removeComments": false,
    "target": "es5",
    "lib": [ "es5", "es2015.core" ],
    "module": "amd",

    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true
  },
  "include": [
    "Trace.d.ts",
    "Main.ts"
  ]
}
