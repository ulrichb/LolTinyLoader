{
  "compilerOptions": {
    "outFile": "bin/Tests.js",
    "removeComments": false,
    "target": "es5",
    "lib": [ "es5", "dom" ],
    "module": "amd",

    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true
  }
}
