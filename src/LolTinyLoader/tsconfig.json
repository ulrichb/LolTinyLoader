{
  "compilerOptions": {
    "outFile": "bin/LolTinyLoader.js",
    "declaration": true,
    "removeComments": false,
    "target": "es5",
    "lib": [ "es5" ],

    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true
  },
  "exclude": [ "bin" ]
}
